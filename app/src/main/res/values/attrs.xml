<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Switch">
        <attr name="leftSwitch" format="string"/>
        <attr name="rightSwitch" format="string"/>
        <attr name="heightSwitch" format="integer"/>
        <attr name="textSizeSwitch" format="integer"/>
        <attr name="drawableRight" format="reference"/>
        <attr name="drawableLeft" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SARIncDecView">
        <attr name="defaultTitle" format="string|reference" />
        <attr name="incrementBy" format="integer" />
        <attr name="minNumber" format="integer" />
        <attr name="maxNumber" format="integer" />
        <attr name="initialNumber" format="integer" />
        <attr name="secondTitle" format="string|reference" />
    </declare-styleable>

    <declare-styleable name="SARNavigationBarView">
        <attr name="navTextColorSelected" format="color|reference"/>
        <attr name="navTintSelected" format="color|reference"/>
        <attr name="navTintNormal" format="color|reference"/>
    </declare-styleable>
</resources>